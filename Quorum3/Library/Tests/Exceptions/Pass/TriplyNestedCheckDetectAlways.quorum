/*
Expected output:
detect 1
detect 2
check 3
always 3
always 2
always 1
*/

//use Libraries.Language.Errors.all

class Main
    action Main
        check
            alert("some error")
        detect e
            output "detect 1"
            check
                alert("detect 2")
            detect j
                output j:GetErrorMessage()
                check
                    output "check 3"
                detect f
                    output "shouldn't be here"
                always
                    output "always 3"
                end
            always
                output "always 2"
            end
        always
            output "always 1"
        end
    end
end