use Libraries.Containers.Table
class Main
    action Main
        Table<integer> t
        t:AddToEndOfRow(0,1)
        t:AddToEndOfRow(0,2)
        t:AddToEndOfRow(0,3)
        t:AddToEndOfRow(0,4)

        integer r1 = t:Get(0,0)
        integer r2 = t:Get(0,1)
        integer r3 = t:Get(0,2)
        integer r4 = t:Get(0,3)
        output r1
        output r2
        output r3
        output r4
    end
end