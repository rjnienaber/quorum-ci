use Libraries.Containers.Array

class Main
    action Main
        Array<integer> a
        a:SetAutoResize(false)
        a:Add(1)
        a:Add(2)
        a:Add(3)

        integer result1 = a:Get(0)
        integer result2 = a:Get(1)
        integer result3 = a:Get(2)

        output result1
        output result2
        output result3
    end
end