class Node<Type>
    Type data = undefined
    Node<Type> next = undefined
    Node<Type> prev = undefined

    action getData() returns Type
        return data
    end

    action setData(Type d)
        data = d
    end

    action setNext(Node<Type> n)
        next = n
    end

    action setPrev(Node<Type> n)
        prev = n
    end

    action getNext() returns Node<Type>
        return next
    end

    action getPrev() returns Node<Type>
        return prev
    end
end