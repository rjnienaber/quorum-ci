use Libraries.Containers.Array
use Libraries.Compute.Random
use Libraries.Compute.Math

Array<integer> a
Array<number> p
Random r
integer n = 1000000
integer max = 5
a:SetSize(max)
p:SetSize(max)
integer i = 0
repeat max times
    a:Set(i, 0)
    p:Set(i, 0)
    i = i + 1
end

repeat n times
    i = r:RandomIntegerBetween(0, max - 1)
    x = a:Get(i)
    a:Set(i, x + 1)
end

Math m
i = 0
repeat max times
    dummyLine = "Goofy Quorum Needing To Insert a Random Line Bug"
    number x = a:Get(i)
    p:Set(i, m:Round(x / n * 100, 0))
    //output a:Get(i) + " | " + p:Get(i)
    i = i + 1
end

pass = true
i = 0
repeat max times
    if cast(integer, p:Get(i)) not= 20
        pass = false
    end
end

if pass
    output "Normal Distribution"
else
    output "Fail Distribution"
end