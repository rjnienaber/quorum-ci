/**
 * Libraries.Compute.Random test case
 *
 * Tests the RandomListOfIntegers() function with a known seed.
 * Expected variable values:
 *
 * size = 5
 * a = 544657192
 * b = 264551322
 * c = 1651751212
 * d = 616702974
 * e = 1943051751
 * empty = true
 *
 */

use Libraries.Containers.List
use Libraries.Containers.Blueprints.Iterator
use Libraries.Compute.Random

class Main
    action Main
        boolean pass = false
        Random random
        random:SetSeed(1101102342)
        List<integer> list = random:RandomListOfIntegers(5)
        Iterator<integer> i = list:GetIterator()
        
        integer size = list:GetSize()
        integer a = i:Next()
        integer b = i:Next()
        integer c = i:Next()
        integer d = i:Next()
        integer e = i:Next()
        boolean empty = not i:HasNext()

        output a
        output b
        output c
        output d
        output e
        output empty
        output size
    end
end