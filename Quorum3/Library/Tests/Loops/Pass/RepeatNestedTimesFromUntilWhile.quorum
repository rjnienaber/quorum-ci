class Main
    action Main
        integer toPrint = 1
        
        repeat 3 times        
            repeat 3 times
                integer un = 0
                repeat until un = 3
                    integer wh = 0
                    repeat while wh < 3
                        toPrint = toPrint + 1
                        wh = wh+ 1
                    end
                    un = un + 1
                end
            end
        end

        output toPrint
    end
end