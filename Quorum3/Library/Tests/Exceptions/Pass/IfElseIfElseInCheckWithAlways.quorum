/*
Expected output:
detect
always
*/

use Libraries.Language.Errors.CastError

class Main
    action Main
        check
            integer k = cast(integer, "3")
            if k = 4
                output "shouldn't be here"
            elseif k = 3
                integer z = cast(integer, "91.2")
            else
                output "shouldn't be here either"
            end
        detect e is CastError
            output "detect"
        always
            output "always"
        end
    end
end