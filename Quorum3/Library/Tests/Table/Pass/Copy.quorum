use Libraries.Containers.Table
use Libraries.Containers.Blueprints.Iterator
use Libraries.Containers.Array
class Main
    action Main
        integer r1
        integer r2
        integer r3
        integer r4
        Table<integer> t
        t:SetSize(1,4)
        t:Set(0,0,1)
        t:Set(0,1,2)
        t:Set(0,2,3)
        t:Set(0,3,4)

        Object o = t:Copy()
        Table<integer> z = cast(Table<integer>, o)
        r1 = z:Get(0,0)
        r2 = z:Get(0,1)
        r3 = z:Get(0,2)
        r4 = z:Get(0,3)
        output r1
        output r2
        output r3
        output r4
    end
end