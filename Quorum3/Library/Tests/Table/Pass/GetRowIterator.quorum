    Author: Melissa Stefik

    results:
            r1=1
            r2=2
            r3=3
            r4=4
*/
use Libraries.Containers.Table
use Libraries.Containers.Blueprints.Iterator
use Libraries.Containers.Array
class Main
    action main
integer r1
integer r2
integer r3
integer r4
        Table<integer> t
        t:SetSize(1,4)
        t:Set(0,0,1)
        t:Set(0,1,2)
        t:Set(0,2,3)
        t:Set(0,3,4)

        Iterator<Array<integer>> it = t:GetRowIterator()
        repeat while it:HasNext()
            Array<integer> row = it:Next()
            r1 = row:Get(0)
            r2 = row:Get(1)
            r3 = row:Get(2)
            r4 = row:Get(3)
        end

        output r1
        output r2
        output r3
        output r4
    end
end