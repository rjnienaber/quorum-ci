/*
Expected output:
detect
always
always
*/

use Libraries.Language.Errors.CastError

class Main
    action Main
        check
            integer k = cast(integer, "3.7")
        detect e is CastError
            output "detect"
        always
            integer k = 0
            repeat while k < 2
                output "always"
                k = k + 1
            end
        end
    end
end