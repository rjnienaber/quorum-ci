use Libraries.Containers.Queue

/**
 * Uses:
 * action Add( Type value )
 * action RemoveAll()
 * action Remove() returns Type
**/

/**
 * Description:
 * The purpose of this test case is to test the RemoveAll action.
**/

/**
 * Expected Results:
 * result = "SixFiveFourTwo"
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       Queue<text> s

       s:Add("One")
       s:Add("Two")
       s:Add("One")
       s:Add("Four")
       s:Add("Five")
       s:Add("Six")

       s:RemoveAll("One")
       text result = ""
       integer i = 0
       repeat 4 times 
            result = result + s:RemoveFromFront()
            i = i + 1
       end

       output result
    end
end