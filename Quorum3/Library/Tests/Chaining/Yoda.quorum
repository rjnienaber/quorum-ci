class Yoda
    boolean isGreen = true
    integer height = 66
    number age = 900.0
    text color = "green"

    action Getboolean returns boolean
        return isGreen
    end

    action GetBoolean returns Boolean
        Boolean obj
        obj:SetValue(isGreen)
        return obj
    end

    action Getinteger returns integer
        return height
    end

    action GetInteger returns Integer
        Integer obj
        obj:SetValue(height)
        return obj
    end

    action Getnumber returns number
        return age
    end

    action GetNumber returns Number
        Number obj
        obj:SetValue(age)
        return obj
    end

    action Gettext returns text
        return color
    end

    action GetText returns Text
        Text obj
        obj:SetValue(color)
        return obj
    end

    action GetObject returns Object
        return cast(Object, me)
    end

    action Getundefined returns Yoda
        return undefined
    end

    action GetYoda returns Yoda
        return me
    end
end
