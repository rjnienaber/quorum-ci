
use Libraries.Containers.Table
class Main
    action main
        Table<integer> t
        t:SetMaxSize(1, 2)
        t:SetAutoResize(false)
        t:Add(0,0,11)
        t:Add(0,1,12)
        t:Add(1,0,21)
        t:Add(1,1,22)

        integer r1 = t:Get(0,0)
        integer r2 = t:Get(0,1)
        integer r3 = t:Get(1,0)
        integer r4 = t:Get(1,1)
        output r1
        output r2
        output r3
        output r4
    end
end