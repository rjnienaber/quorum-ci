use Libraries.Containers.List

/**
 * Uses:
 * action Size() returns integer
 * action Add( Type value)
**/

/**
 * Description:
 * the purpose of the test case is to test the Size action.  Before and after
 * each item is inserted into the list the size is calculated.
**/

/**
 * Expected Results:
 * integer textSize = 28
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       List<text> textList

       integer textSize = textList:GetSize()

       textList:Add("one")
       textSize = textSize + textList:GetSize()
       textList:Add("five")
       textSize = textSize + textList:GetSize()
       textList:Add("nine")
       textSize = textSize + textList:GetSize()
       textList:Add("one")
       textSize = textSize + textList:GetSize()
       textList:Add("eight")
       textSize = textSize + textList:GetSize()
       textList:Add("three")
       textSize = textSize + textList:GetSize()
       textList:Add("one")
       textSize = textSize + textList:GetSize()

       output textSize
    end
end