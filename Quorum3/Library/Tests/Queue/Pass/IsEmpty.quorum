use Libraries.Containers.Queue

/**
 * Uses:
 * action AddToTop( Type value )
 * action IsEmpty() returns boolean
**/

/**
 * Description:
 * this test case tests the IsEmpty action.
**/

/**
 * Expected Results:
 * bool1 = false
 * bool2 = true
**/

/*
 * @author Elliot Motl
*/
class Main
    action Main
       Queue<text> s1
       Queue<text> s2
       s1:Add("One")
       s1:Add("Two")
       s1:Add("Three")
       s1:Add("Four")
       s1:Add("Five")

       boolean bool1 = s1:IsEmpty()
       boolean bool2 = s2:IsEmpty()

       output bool1
       output bool2
    end
end