use Libraries.Containers.Table
class Main
    action main
        Table<integer> t
        t:SetSize(1,4)
        t:Set(0,0,1)
        t:Set(0,1,2)
        t:Set(0,2,4)
        t:Set(0,3,2)

        boolean r1 = t:Remove(2)
        integer r2 = t:Get(0,0)
        integer r3 = t:Get(0,1)
        integer r4 = t:Get(0,2)
        output r1
        output r2
        output r3
        output r4
    end
end