use Libraries.Containers.Table
use Libraries.Containers.Array
class Main
    action main
        Table<integer> t
        Array<integer> ar
        t:Add(0,0,4)
        t:Add(0,0,3)
        t:Add(0,0,2)
        t:Add(0,0,1)

        ar = t:GetRow(0)
        integer r1 = ar:Get(0)
        integer r2 = ar:Get(1)
        integer r3 = ar:Get(2)
        integer r4 = ar:Get(3)
        integer r5 = ar:Get(4)
        output r1
        output r2
        output r3
        output r4
    end
end