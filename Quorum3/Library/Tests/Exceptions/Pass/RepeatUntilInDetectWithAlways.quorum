/*
Expected output:
detect
detect
always
*/

use Libraries.Language.Errors.CastError

class Main
    action Main
        check
            integer k = cast(integer, "3.7")
        detect e is CastError
            integer z = 0
            repeat until z = 2
                output "detect"
                z = z + 1
            end
        always
            output "always"
        end
    end
end