use Libraries.Containers.Table
class Main
    action main
        Table<integer> t
        t:SetMaxSize(3, 5)

        integer r1 = t:GetMaxNumberOfColumns()
        integer r2 = t:GetMaxNumberOfRows()
        output r1
        output r2
    end
end