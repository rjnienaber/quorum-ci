use Libraries.Containers.Table
class Main
    action Main
        Table<integer> t
        t:SetSize(3, 5)

        integer r1 = t:GetNumberOfRows()
        integer r2 = t:GetSizeOfRow(0)
        integer r3 = t:GetSizeOfRow(1)
        integer r4 = t:GetSizeOfRow(2)
        output r1
        output r2
        output r3
        output r4
    end
end